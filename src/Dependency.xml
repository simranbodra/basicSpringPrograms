<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:annotation-config />

	<bean id="employee"
		class="com.bridgelabz.autowiredannotationprograms.model.Employee">
		<property name="id" value="123456"></property>
		<property name="name" value="Simran Bodra"></property>
		<property name="email" value="simranbodra6@gmail.com"></property>
	</bean>

	<bean id="pancard"
		class="com.bridgelabz.autowiredannotationprograms.model.Pancard">
		<property name="pancardHolderName" value="Simran Bodra"></property>
		<property name="pancardNo" value="SB123456"></property>
	</bean>
</beans>