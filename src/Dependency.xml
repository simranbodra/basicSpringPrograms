<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
               
<bean id="employee"
		class="com.bridgelabz.autowirebyconstructor.model.Employee"
		scope="prototype" autowire = "constructor">
		<constructor-arg name="id" value="123456"></constructor-arg>
		<constructor-arg name="name" value="Simran Bodra"></constructor-arg>
		<constructor-arg name="email" value="simranbodra6@gmail.com"></constructor-arg>
		
	</bean>

	<bean id="pancard1"
		class="com.bridgelabz.autowirebyconstructor.model.Pancard">
		<property name="pancardHolderName" value="Simran"></property>
		<property name="pancardNo" value="SB123456"></property>
	</bean>
</beans>